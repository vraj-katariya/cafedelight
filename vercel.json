{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/frontend/browser",
    "framework": "angular",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}